{
  "name": "miconfont",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.0.2",
  "description": "A tool for maintaining icons like iconfont",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ListeningRift/Miconfont.git"
  },
  "bugs": "https://github.com/ListeningRift/Miconfont/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "simple-git-hooks",
    "dev": "unbuild --stub",
    "cli": "esno src/generate.ts",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "build": "unbuild",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "svg2ttf": "^6.0.3",
    "svgicons2svgfont": "^14.0.2",
    "svgo": "^3.3.2",
    "ttf2woff": "^3.0.0",
    "ttf2woff2": "^6.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.14.0",
    "@types/node": "^20.12.7",
    "@types/svg2ttf": "^5.0.3",
    "@types/ttf2woff": "^2.0.4",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.1",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.0.2",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.9",
    "vitest": "^1.5.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
